{
	"name": "LuaCache",
	"version": "1.0.0",
	"author": [
		"Robert Nix"
	],
	"url": "https://gitlab.com/HydraWiki/extensions/LuaCache",
	"descriptionmsg": "luacache-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.35.0",
		"extensions": {
			"Scribunto": "*"
		}
	},
	"AvailableRights": [
		"luacachecanexpand",
		"luacacheconsole"
	],
	"GroupPermissions": {
		"user": {
			"luacachecanexpand": true
		},
		"sysop": {
			"luacacheconsole": true
		}
	},
	"MessagesDirs": {
		"LuaCache": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\LuaCache\\": "src/"
	},
	"HookHandlers": {
		"ScribuntoHooks": {
			"class": "MediaWiki\\Extension\\LuaCache\\ScribuntoHooks"
		}
	},
	"Hooks": {
		"ScribuntoExternalLibraries": "ScribuntoHooks"
	},
	"manifest_version": 2
}
